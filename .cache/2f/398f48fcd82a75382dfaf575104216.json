App.js","original":{"line":48,"column":29}},{"generated":{"line":115,"column":19},"source":"projects/music-master/components/App.js","original":{"line":48,"column":38}},{"generated":{"line":115,"column":21},"source":"projects/music-master/components/App.js","original":{"line":48,"column":40}},{"generated":{"line":116,"column":0},"source":"projects/music-master/components/App.js","original":{"line":48,"column":28}},{"generated":{"line":117,"column":0},"source":"projects/music-master/components/App.js","original":{"line":48,"column":16}},{"generated":{"line":117,"column":44},"source":"projects/music-master/components/App.js","original":{"line":42,"column":12}},{"generated":{"line":117,"column":45},"source":"projects/music-master/components/App.js","original":{"line":41,"column":8}},{"generated":{"line":118,"column":0},"source":"projects/music-master/components/App.js","original":{"line":54,"column":5}},{"name":"Component","generated":{"line":122,"column":2},"source":"projects/music-master/components/App.js","original":{"line":7,"column":18}},{"name":"App","generated":{"line":124,"column":15},"source":"projects/music-master/components/App.js","original":{"line":56,"column":15}}],"sources":{"projects/music-master/components/App.js":"import React, {Component} from 'react';\nimport Artist from './Artist';\nimport Tracks from './Tracks';\nimport Search from './Search';\n\nconst API_ADDRESS = 'https://spotify-api-wrapper.appspot.com';\nclass App extends Component {\n    state = { artist: null, tracks: [] };\n\n    componentDidMount() {\n        this.searchArtist('fally');\n    }\n\n    searchArtist = artistQuery => {\n        //console.log('this.state', this.state);\n        fetch(`${API_ADDRESS}/artist/${artistQuery}`)\n        .then(response => response.json())\n        .then(json => {\n            //console.log('json', json);\n            if (json.artists.total > 0) {\n                const artist = json.artists.items[0];\n\n                //console.log('artist', artist);\n                this.setState({ artist })\n\n                fetch(`${API_ADDRESS}/artist/${artist.id}/top-tracks`)\n                .then(response => response.json())\n                .then(json => \n                    //console.log('top-tracks', json)\n                    this.setState({ tracks: json.tracks }))\n                .catch(error => alert(error.message));\n            }\n        })\n        .catch(error => alert(error.message));\n\n\n    }\n\n    render () {\n        //console.log('this.state', this.state)\n        return (\n            <div>\n                <h2>Kingopaty Music</h2>\n                <Search searchArtist={this.searchArtist}/>\n                <Artist artist={ this.state.artist }/>\n                <Tracks tracks={ this.state.tracks }/>\n                <hr />\n                <div style={{marginTop: 40}}>\n                    Copyrigth: Patrick Rashidi, 2019\n                </div>\n            </div>\n            \n        );\n    }\n}\nexport default App;"},"lineCount":null}},"error":null,"hash":"4642ca9a21c1c55343e0a1807f1472cb","cacheData":{"env":{}}}